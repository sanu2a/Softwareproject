class twoD {
 int x;
 int y;
 void transl(int vecteur){
   x = x + vecteur;
   y = y + vecteur;
}
void mult(int a){
  x = x*a;
  y = y*a;

}
void toString(){
  println("x= ",x, " y= ",y);  
}

}


